{
  "title": "Bee & Bloom",
  "name": "bee-and-bloom",
  "description": "A website for two beekeepers",
  "version": "0.0.1",
  "dependencies": {},
  "devDependencies": {
    "autoprefixer": "^5.2.0",
    "babel-loader": "^5.4.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chokidar-cli": "^1.0.1",
    "eslint": "^1.3.1",
    "expose-loader": "^0.7.4",
    "fg-loadcss": "^0.2.4",
    "fg-loadjs": "^0.2.3",
    "imports-loader": "^0.6.5",
    "isotope-layout": "3.0.2",
    "jsdoc": "^3.5.5",
    "jsdoc-oblivion": "0.0.4",
    "parallelshell": "^2.0.0",
    "paramalama": "~0.2",
    "postcss-cli": "^2.6.0",
    "properjs-controller": "~0.2",
    "properjs-debounce": "^0.2.0",
    "properjs-easing": "~0.2",
    "properjs-imageloader": "^0.3.1",
    "properjs-matchelement": "~0.2",
    "properjs-mediabox": "~0.3",
    "properjs-pagecontroller": "^0.3.11",
    "properjs-resizecontroller": "^0.2.2",
    "properjs-scroll2": "~0.2",
    "properjs-scrollcontroller": "^0.2.2",
    "properjs-template": "~0.2",
    "properjs-throttle": "^0.3.0",
    "properjs-tween": "~0.2",
    "susy": "^2.2.12",
    "webpack": "^1.15.0"
  },
  "config": {
    "scss_file": "./sass/screen.scss",
    "css_file": "./web_template/css/screen.css",
    "js_file": "./web_template/js/app.js",
    "js_map": "./web_template/js/app.js.map",
    "js_map_min": "./web_template/js/app.min.js.map",
    "sqs_css_file": "./template/styles/screen.css",
    "sqs_js_file": "./template/scripts/app-sqs.js",
    "sqs_js_map": "./template/scripts/app.js.map",
    "sqs_js_map_min": "./template/scripts/app.min.js.map"
  },
  "scripts": {
    "doc:js": "jsdoc js_src/ -r -R README.md -P package.json -t node_modules/jsdoc-oblivion/template -c node_modules/jsdoc-oblivion/template/jsdoc.conf.json -d js_docs",
    "lint:js": "eslint js_src",
    "build": "npm run build:js && npm run build:css",
    "prod:js": "webpack --optimize-minimize",
    "build:js": "webpack -d",
    "build:css": "sass --scss --style compressed $npm_package_config_scss_file:$npm_package_config_sqs_css_file",
    "postbuild:css": "postcss --use autoprefixer --autoprefixer.browsers 'last 3 versions' --output $npm_package_config_css_file $npm_package_config_css_file",
    "watch": "parallelshell 'npm run watch:js' 'npm run watch:sass'",
    "watch:js": "chokidar ./js_src/* --initial -d 10 -c 'npm run build:js'",
    "watch:sass": "chokidar ./sass/* --initial -d 10 -c 'npm run build:css'",
    "server": "squarespace-server https://bee-and-bloom.squarespace.com --auth",
    "start": "npm run watch"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/jordanegstad/bee-and-bloom"
  },
  "keywords": [],
  "license": "MIT",
  "contributors": [
    {
      "name": "Jordan Egstad",
      "email": "jordan@egstad.com",
      "url": "http://egstad.com"
    },
    {
      "name": "Brandon Kitajchuk",
      "email": "kitajchuk@gmail.com",
      "url": "http://blkpdx.com"
    }
  ],
  "readmeFilename": "README.md"
}
